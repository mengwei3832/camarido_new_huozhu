<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    
    android:descendantFocusability="blocksDescendants" >
<RelativeLayout 
    android:background="@drawable/historty_yun_dan_bg"
    android:layout_marginTop="10dp"
    android:layout_marginLeft="10dp"
    android:layout_marginRight="10dp"
    android:paddingBottom="10dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <ImageView
        android:id="@+id/iv_qi_shi_dian"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:contentDescription="@string/_image"
        android:src="@drawable/historty_yun_dan_qidian" />

    <TextView
        android:id="@+id/tv_history_ding_dan_start_palce_hint"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/iv_qi_shi_dian"
        android:layout_marginTop="1dp"
        android:layout_toEndOf="@id/iv_qi_shi_dian"
        android:layout_toRightOf="@id/iv_qi_shi_dian"
        android:text="@string/tv_history_yun_dan_start_palce_hint"
        android:textColor="#2299EE"
        android:textSize="13sp" />

    <TextView
        android:id="@+id/tv_history_ding_dan_start_palce"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/tv_history_ding_dan_start_palce_hint"
        android:layout_toEndOf="@id/tv_history_ding_dan_start_palce_hint"
        android:layout_toRightOf="@id/tv_history_ding_dan_start_palce_hint"
        android:ellipsize="end"
        android:singleLine="true"
        android:textColor="#303030"
        android:textSize="13sp" />

    <View
        android:id="@+id/v_short_line"
        android:layout_width="198dp"
        android:layout_height="1dp"
        android:layout_alignLeft="@id/iv_qi_shi_dian"
        android:layout_alignStart="@id/iv_qi_shi_dian"
        android:layout_below="@id/iv_qi_shi_dian"
        android:background="#55BBFF" />

    <ImageView
        android:id="@+id/iv_mu_di_di"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/v_short_line"
        android:layout_margin="10dp"
        android:contentDescription="@string/_image"
        android:src="@drawable/historty_yun_dan_zhongdian" />

    <TextView
        android:id="@+id/tv_history_ding_dan_end_palce_hint"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/iv_mu_di_di"
        android:layout_marginTop="1dp"
        android:layout_toEndOf="@id/iv_mu_di_di"
        android:layout_toRightOf="@id/iv_mu_di_di"
        android:text="@string/tv_history_yun_dan_end_palce_hint"
        android:textColor="#2299EE"
        android:textSize="13sp" />

    <TextView
        android:id="@+id/tv_history_ding_dan_end_palce"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/tv_history_ding_dan_end_palce_hint"
        android:layout_toEndOf="@id/tv_history_ding_dan_end_palce_hint"
        android:layout_toRightOf="@id/tv_history_ding_dan_end_palce_hint"
        android:ellipsize="end"
        android:singleLine="true"
        android:textColor="#303030"
        android:textSize="13sp" />

    <View
        android:id="@+id/v_long_line"
        android:layout_width="208dp"
        android:layout_height="1dp"
        android:layout_below="@id/iv_mu_di_di"
        android:background="#55BBFF" />

    <TextView
        android:id="@+id/tv_hitory_ding_dan_time"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/v_long_line"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="10dp"
        android:textColor="#303030"
        android:textSize="12sp" />

    <View
        android:id="@+id/v_short_one"
        android:layout_width="1dp"
        android:layout_height="18dp"
        android:layout_alignTop="@id/tv_hitory_ding_dan_time"
        android:layout_toEndOf="@id/tv_hitory_ding_dan_time"
        android:layout_toRightOf="@id/tv_hitory_ding_dan_time"
        android:background="#55BBFF" />

    <TextView
        android:id="@+id/tv_hitory_ding_dan_goods_type"
        android:layout_width="41dp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/v_short_one"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_toEndOf="@id/v_short_one"
        android:layout_toRightOf="@id/v_short_one"
        android:textColor="#303030"
        android:gravity="center"
        android:textSize="12sp" />

    <View
        android:id="@+id/v_short_two"
        android:layout_width="1dp"
        android:layout_height="18dp"
        android:layout_alignTop="@id/tv_hitory_ding_dan_goods_type"
        android:layout_toEndOf="@id/tv_hitory_ding_dan_goods_type"
        android:layout_toRightOf="@id/tv_hitory_ding_dan_goods_type"
        android:background="#55BBFF" />

    <TextView
        android:id="@+id/tv_hitory_ding_dan_car_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/v_short_two"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_toEndOf="@id/v_short_two"
        android:layout_toRightOf="@id/v_short_two"
        android:textColor="#303030"
        android:textSize="12sp" />

    <Button
        android:id="@+id/btn_hitory_ding_dan_status"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_marginEnd="25dp"
        android:layout_marginRight="25dp"
        android:background="@drawable/historty_yun_dan_btn_bg"
        android:textColor="#FFFFFF"
        android:textSize="15sp" />

    <ImageView
        android:id="@+id/iv_historty_ding_dan_insurance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:clickable="false"
        android:contentDescription="@string/_image"
        android:focusable="false"
        android:src="@drawable/historty_yun_dan_insurance_icon" />
</RelativeLayout>
</RelativeLayout>