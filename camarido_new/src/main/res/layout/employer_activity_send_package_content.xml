                android:textSize="14sp"
                android:textColor="#333"
                android:layout_toRightOf="@id/cb_send_insurance"
                android:layout_alignBottom="@+id/cb_send_insurance"
                android:id="@+id/tv_send_insurance"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/tv_send_insurance" 
                android:layout_marginLeft="5dp"/>
            <TextView 
                android:layout_toRightOf="@id/tv_send_insurance"
                android:layout_alignBottom="@+id/cb_send_insurance"
                android:textColor="#666"
                android:textSize="14sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/tv_send_insurance_intro"/>
            <TextView 
                android:textSize="13sp"
                android:layout_marginTop="10dp"
                android:layout_below="@+id/cb_send_insurance"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/tv_send_insurance_note"
                android:textColor="#FF0000"/>
        </RelativeLayout>
    </LinearLayout>
		
